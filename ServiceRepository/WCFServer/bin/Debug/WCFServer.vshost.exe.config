<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="serviceRepoAddress" value= "net.tcp://localhost:41234/IServiceRepository" />
    </appSettings>
  <system.serviceModel>
  <behaviors>
    <serviceBehaviors>
      <behavior name="EmployeeManager_Behavior">
        <serviceDebug includeExceptionDetailInFaults="true"/>
        <serviceMetadata httpGetEnabled="true" />
      </behavior>
    </serviceBehaviors>

  </behaviors>
  </system.serviceModel>
</configuration>